{
    "name": "laraone/laraone",
    "description": "Laraone CMS",
    "keywords": ["laraone", "cms", "responsive", "web-design", "themes", "php", "javascript", "components"],
    "type": "project",
    "homepage": "https://laraone.com",
    "authors": [
        {
            "name": "Niko Craft"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.6.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "2.5.1",
        "laraone/themes-support": "^1.0.0",
        "laravel/helpers": "^1.1.1",
        "mews/purifier": "3.1.2",
        "santigarcor/laratrust": "5.2.*",
        "silber/page-cache": "^1.0",
        "spatie/laravel-schemaless-attributes": "^1.4",
        "tightenco/ziggy": "0.8.*",
        "nesbot/carbon": "^2.27.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.6",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeds\\Updates\\": "database/seeds/updates",
            "Database\\Themes\\": "database/themes"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
